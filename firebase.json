{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "runtime": "nodejs18",
      "v1": true
    }
  ],
  "hosting": {
    "target": "integra-rh",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/asignarPruebasPsicometricas",
        "function": "asignarPruebasPsicometricas"
      },
      {
        "source": "/reenviarInvitacion",
        "function": "reenviarInvitacion"
      },
      {
        "source": "/manageUserRole",
        "function": "manageUserRole"
      },
      {
        "source": "/listUsers",
        "function": "listUsers"
      },
      {
        "source": "/listUsersHttp",
        "function": "listUsersHttp"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "storage": { "rules": "storage.rules" }
}
